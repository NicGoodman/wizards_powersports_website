{
  "require": {
    "craftcms/cms": "^3.4.0",
    "craftcms/redactor": "^2.6",
    "elivz/craft-single-cat": "^1.2",
    "monachilada/craft-matrixtoolbar": "^1.0",
    "nystudio107/craft-imageoptimize": "^1.6",
    "nystudio107/craft-seomatic": "^3.2",
    "olivestudio/craft-olivemenus": "^1.1",
    "verbb/field-manager": "^2.1",
    "verbb/super-table": "^2.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
